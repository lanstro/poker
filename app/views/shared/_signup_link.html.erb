<% if signed_in? %>
	<% current_table=false %>
	<% if current_table %>
		<%= link_to "Return to table", table_path(current_table.name), class: "btn btn-large btn-primary" %>
	<% else %>
		<%= link_to "Find a game now!", tables_path, class: "btn btn-large btn-primary" %>
	<% end %>
<% else %> 
	<%= link_to "Sign up now!", new_user_path, class: "btn btn-large btn-primary" %>
<% end %>